{
  "name": "exam-study-planner",
  "version": "1.0.0",
  "description": "Full-stack Exam Study Planner - Configurable for multiple exam types",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "build:dev": "cd client && npm run build:dev",
    "install-client": "cd client && npm install",
    "install-all": "npm install && npm run install-client",
    "heroku-postbuild": "npm run install-client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "exam",
    "planner",
    "",
    "study",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "nodemailer": "^6.9.4",
    "cloudinary": "^1.40.0",
    "moment": "^2.29.4",
    "cron": "^2.4.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}
